force_silent	chgrp.c	/^static int force_silent;$/;"	v	file:
groupname	chgrp.c	/^static char *groupname;$/;"	v	file:
long_options	chgrp.c	/^static struct option long_options[] =$/;"	v	typeref:struct:option	file:
main	chgrp.c	/^int main(int argc, char **argv)$/;"	f
myisnumber	chgrp.c	/^int myisnumber(char *str)$/;"	f
parse_group	chgrp.c	/^void parse_group(char *name, int *g)$/;"	f
program_name	chgrp.c	/^char *program_name;$/;"	v
recurese	chgrp.c	/^static int recurese;$/;"	v	file:
